<%=# form_for [@project, @gollum_wiki], :url => { :controller => 'gollum_wikis', :action=> 'update', :project_id => @project } do |f| %>
<%= debug @project %>
<%= debug @gollum_wiki %>


<div class="box tabular">
  <p>
    <%= label(:gollum_wiki, :git_path) %>
    <%= f.text_field :git_path %>
  </p>
  <p>
    <%= label(:gollum_wiki, :markup_language) %>
    <%= f.select(:markup_language, options_for_select(Gollum::Markup::formats.keys, @gollum_wiki.markup_language)) %></p>
  </p>
  <p>
    <%= label(:gollum_wiki, :page_files_directory) %>
    <%= f.text_field :directory %>
  </p>
</div>

  <p><%= f.submit %></p>
<% end %>
